<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pedroribeiro.intellicity">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/map"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/map"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RegistarUtilizadorActivity"></activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyD57dIqodIjf-XMwoGPRdGfuU7PTvJ_2iQ" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NotasActivity" />
        <activity android:name=".Second" />
        <activity android:name=".MapActivity" />
        <activity android:name=".StreetViewActivity" />

        <service
            android:name=".FetchAddressIntentService"
            android:exported="false" />
        <service
            android:name=".FetchCoordinatesIntentService"
            android:exported="false" />
        <service android:name=".GeofenceTransitionsIntentService" />
    </application>

</manifest>